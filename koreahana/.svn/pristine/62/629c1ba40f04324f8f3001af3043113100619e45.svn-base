$(document).ready(function(){
	$('#statDiv').html($('#percentDiv').html());
});

//페이징
function fnPage(p){
	$("#pageIndex").val(p);
	$("#listPageForm").submit();
}

//상세
function fnView(sprtSn){
	$("#kepMode").val('view');
	$("#sprtSn").val(sprtSn);
	$("#writePageForm").submit();
}


//추가/수정
function fnWrite(sprtSn){
	$("#kepMode").val('write');
	$("#sprtSn").val(sprtSn);
	$("#writePageForm").submit();
}
