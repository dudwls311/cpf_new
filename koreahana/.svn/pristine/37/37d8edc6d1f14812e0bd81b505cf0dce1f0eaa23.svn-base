.st_radio, .st_check, .st_input {position: relative;color: #333;display: inline-block;}
.st_input button.ico_search {position: absolute;right: 0;top: 0;}
.st_input button.ico_search {display: inline-block;width: 30px;height: 30px;border: none;cursor: pointer;background: url(/support/img/common/icon_search_03.png) center center no-repeat;}
.st_input input {background: #fff;border: none;padding: 6px 5px 7px 5px;border: 1px solid #ededed;border-bottom: 1px solid #ddd;border-radius: 3px;outline: none;font-size: 14px;}
.st_input input:focus {border-bottom: 1px solid #619e0b;}
.st_input label,  .st_radio label:first-child {margin-right: 10px;margin-left: 24px;}
.st_check label {margin-right: 5px;}
.st_select {display: inline-block;color: #333;}
.st_select label {margin-right: 10px;margin-left: 24px;}
.st_select select {background: #fff;border: none;height: 30px;padding: 0px 10px;border: 1px solid #ededed;border-bottom: 1px solid #ddd;border-radius: 3px;}
.st_radio {position: relative;display: inline-block;font-weight: 600;color: #333;padding-top: 5px;}

.st_check input[type=checkbox]{display: none;}
.st_check input[type=checkbox] +label {position: relative;cursor:pointer;font-weight: 400;color: #999;}
.st_check input[type=checkbox] +label:before{content:"";display: inline-block;cursor: pointer;height: 19px;width: 19px;margin: 0 5px 0 10px;background: #fff;border: 1px solid #cdcdcd;vertical-align: middle;border-radius: 3px;line-height: 24px;text-align: center;font-weight:bold;font-size: 13px;}
.st_check input[type=checkbox]:checked +label:before{background: #fff url(/support/img/common/icon_check.png) center center  no-repeat;background-size: 14px;border: 1px solid #619e0b;}
.st_check input[type=checkbox]:checked +label{color: #333333;}
.st_check input[type=checkbox]:checked +label:after {background: #ffffff;}

.st_radio input[type=radio]{display: none;}
.st_radio input[type=radio] +label {position: relative;cursor:pointer;font-weight: 400;color: #999;}
.st_radio input[type=radio] +label:before{content:"";display: inline-block;cursor: pointer;height: 19px;width: 19px;margin: 0 5px 0 10px;background: #fff;border: 1px solid #cdcdcd;vertical-align: middle;border-radius: 25px;line-height: 24px;text-align: center;font-weight:bold;font-size: 13px;}
.st_radio input[type=radio] +label:after{content:"";position: absolute;display: inline-block;left: 6px;top: 5px;height: 9px;width: 9px;margin: 0 5px 0 10px;background: #e1e1e1;vertical-align: middle;border-radius: 25px;line-height: 24px;text-align: center;font-weight:bold;font-size:13px;}
.st_radio input[type=radio]:checked +label:before{background-color: #619e0b;border: 1px solid #59910b;}
.st_radio input[type=radio]:checked +label{color: #333333;}
.st_radio input[type=radio]:checked +label:after {background: #ffffff;}

.input_long {display: block;}
.input_long input {width: 100%;}

