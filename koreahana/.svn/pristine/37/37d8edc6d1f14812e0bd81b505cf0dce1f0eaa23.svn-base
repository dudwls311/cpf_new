/*툴팁*/
.tooltip_txt {position: absolute;right: 0;font-size: 14px;margin-top: -30px;line-height: 20px;}
.tooltip_txt:hover .tooltip_pop {display:block;}
.tooltip_txt .tooltip_pop {display:none;position: absolute;top: 35px;right: 0px;width: 340px;padding: 4px 10px 10px;background: #ffffff;border: 1px solid #ccc;border-radius: 5px;z-index: 1;font-size: 12px;font-weight: normal;box-shadow: 1px 1px 2px 1px #82828266;}
.tooltip_txt .tooltip_pop:before {position: absolute;background-color: #ffffff;box-shadow: -2px -2px 2px 0 rgb(178 178 178 / 40%);content: "\00a0";display: block;height: 20px;right: 15px;top: -10px;transform:rotate( 45deg );-moz-transform:rotate( 45deg );-ms-transform:rotate( 45deg );-o-transform:rotate( 45deg );-webkit-transform: rotate( 45deg );width:  20px;}
.tooltip_txt .tooltip_pop b {font-size: 14px;line-height: 30px;color: #22688d;}
